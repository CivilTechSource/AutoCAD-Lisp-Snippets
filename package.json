{
  "name": "autocad-lisp-snippets",
  "displayName": "AutoCAD Lisp Snippets",
  "description": "A lightweight VS Code extension providing useful AutoCAD Lisp code snippets for faster development. Includes header templates, comment blocks, and common function patterns.",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Snippets"
  ],
  "publisher": "CivilTechSource",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CivilTechSource/AutoCAD-Lisp-Snippets.git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "autolisp",
        "path": "./snippets/documentation.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/documentation.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/functions.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/functions.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/sections.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/sections.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/layers.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/layers.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/drawing.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/drawing.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/input-output.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/input-output.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/utilities.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/utilities.code-snippets"
      },
      {
        "language": "autolisp",
        "path": "./snippets/dcl.code-snippets"
      },
      {
        "language": "lisp",
        "path": "./snippets/dcl.code-snippets"
      }
    ]
  }
}
